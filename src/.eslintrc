{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": 0,
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "arrow-parens":"off",
    "operator-linebreak":"off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}